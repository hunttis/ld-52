  "compilerOptions": {
    "strict": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"]
    },
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "ES2021",
    "outDir": "dist"
  },
  "include": ["src"]
}
